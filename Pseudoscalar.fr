M$PseudoscalarFields = {
  S[4] == {
    ClassName        -> sc,
    SelfConjugate    -> True,
    Mass             -> {Msc, 50.0},
    Width            -> {Wsc, 1.0},
    PropagatorLabel  -> "sc",
    PropagatorType   -> D,
    PropagatorArrow  -> None,
    ParticleName     -> "o",
    FullName         -> "o"
  }
}

M$ParametersPseudoscalar = {

  Ct == {
    ParameterType -> External,
    BlockName     -> SIGMA,
    Value         -> 2.2,
    Description   -> "Coeff of the SM top with sigma"
},

  Cf == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> (2.2*vev/Sqrt[2]), (* the vev/s2 cancels the contribs from the Yuk coupling *)
    Description   -> "Coeff of the SM ferms with sigma"
},

  fa == {
    ParameterType -> External,
    BlockName     -> SIGMA,
    Value         -> 2.2,
    Description   -> "Inv coeff of the SM ferms with sigma"
},

  Kg == {
    ParameterType -> External,
    BlockName     -> SIGMA,
    Value         -> -7.2,
    Description   -> "Coeff of the gauge fields with sigma"
},

  Kw == {
    ParameterType -> External,
    BlockName     -> SIGMA,
    Value         -> 7.6,
    Description   -> "Coeff of the gauge fields with sigma"
},

  Kb == {
    ParameterType -> External,
    BlockName     -> SIGMA,
    Value         -> 2.8,
    Description   -> "Coeff of the gauge fields with sigma"
},

  kV == {
    ParameterType -> External,
    BlockName     -> SIGMA,
    Value         -> 1,
    Description   -> "Coupling of Higgs to vectors"
},

  kt == {
    ParameterType -> External,
    BlockName     -> SIGMA,
    Value         -> 1,
    Description   -> "Coupling of Higgs to tops"
},

  Lambda == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> {4 Pi fa},
    Description   -> "Log divergence"
},

  gA == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> (-gw/(4 cw)),
    Description   -> "Log divergence"
},

  CGa == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> (gs^2 Kg)/(16 Pi^2 fa),
    Description   -> "Coefficient of L_haa"
},

  CWa == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> (gw^2 Kw)/(16 Pi^2 fa),
    Description   -> "Coefficient of L_haa"
},

  CBa == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> (g1^2 Kb)/(16 Pi^2 fa),
    Description   -> "Coefficient of L_haa"
},

  Chaa == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> (3 Ct^2 ymt^2 kt)/(8 Pi^2 fa^2 vev) Log[ (Lambda^2)/(ymt^2)],
    Description   -> "Coefficient of L_haa"
},

  ChZa == {
    ParameterType -> Internal,
    BlockName     -> SIGMA,
    Value         -> (3 Ct^2 ymt^2 gA)/(2 Pi^2 fa vev) (kt - kV) Log[ (Lambda^2)/(ymt^2)],
    Description   -> "Coefficient of LhZa"
}
}
